using Extreme.Mathematics;

namespace RecipeSoftware
{
internal class BModel
{
public static string modelB1 = "29.294 + 819.8383 * x - 32.8641 * y";
public static string modelB2 = "-43.621 + 2811.9621 * y + 365.5409 * x - 8351.5551 * Pow(y, 2) - 2275.8001 * x * y + 2564.79 * Pow(x, 2)";

public static void GetBModelOne(double value, ref Vector<double> x, ref Vector<double> f)
{
double num = 29.294 - value;
f[2] = num + 819.8383 * x[0] - 32.8641 * x[1];
}

public static void GetBJacobianOne(ref Vector<double> x, ref Matrix<double> J)
{
J[2, 0] = 819.8383;
J[2, 1] = -32.8641;
}

public static void GetBModelTwo(double value, ref Vector<double> x, ref Vector<double> f)
{
double num = -43.621 - value;
f[2] = num + 2811.9621 * x[1] + 365.5409 * x[0] - 8351.5551 * x[1] * x[1] - 2275.8001 * x[0] * x[1] + 2564.79 * x[0] * x[0];
}

public static void GetBJacobianTwo(ref Vector<double> x, ref Matrix<double> J)
{
J[2, 0] = 5129.58 * x[0] - 2275.8001 * x[1] + 365.5409;
J[2, 1] = -2275.8001 * x[0] - 16703.1102 * x[1] + 2881.9621;
}
}
}


using Extreme.Mathematics;
using Extreme.Mathematics.Optimization;
using NCalc;
using System;
using System.Collections.Generic;
using System.Threading.Tasks;

namespace RecipeSoftware
{
internal class Controller
{
public double saleX = 100.0;
public double saleY = 100.0;
public double val_F = 0.0;
public double val_P = 0.0;
public double val_B = 0.0;

public async Task<LeastSquaresOptimizer> FindSolutionAsync()
{
LeastSquaresOptimizer solutionAsync = await Task.Run<LeastSquaresOptimizer>((Func<LeastSquaresOptimizer>) (() => this.FindSolution()));
return solutionAsync;
}

public LeastSquaresOptimizer FindSolution()
{
LeastSquaresOptimizer solution = (LeastSquaresOptimizer) new TrustRegionReflectiveOptimizer();
solution.ObjectiveFunction = new Func<Vector<double>, Vector<double>, Vector<double»(this.ObjectiveFunction);
solution.JacobianFunction = new Func<Vector<double>, Matrix<double>, Matrix<double»(this.JacobianFunction);
solution.InitialGuess = (Vector<double>) Vector.Create<double>(0.0001, 0.0001);
solution.LowerBounds = (Vector<double>) Vector.Create<double>(1E-05, 1E-05);
solution.UpperBounds = (Vector<double>) Vector.Create<double>(2.0, 2.0);
solution.FindMinimum();
return solution;
}

private Vector<double> ObjectiveFunction(Vector<double> x, Vector<double> f)
{
if (f == (Vector<double>) null)
f = (Vector<double>) Vector.Create<double>(3);
if (this.val_F <= 80.0)
FModel.GetFModelOne(this.val_F, ref x, ref f);
if (this.val_F > 80.0 && this.val_F <= 380.0)
FModel.GetFModelTwo(this.val_F, ref x, ref f);
if (this.val_F > 380.0)
FModel.GetFModelThree(this.val_F, ref x, ref f);
if (this.val_P <= 240.0)
PModel.GetPModelOne(this.val_P, ref x, ref f);
if (this.val_P > 240.0 && this.val_P <= 350.0)
PModel.GetPModelTwo(this.val_P, ref x, ref f);
if (this.val_P > 350.0)
PModel.GetPModelThree(this.val_P, ref x, ref f);
if (this.val_B <= 240.0)
BModel.GetBModelOne(this.val_B, ref x, ref f);
if (this.val_B > 240.0)
BModel.GetBModelTwo(this.val_B, ref x, ref f);
return f;
}

private Matrix<double> JacobianFunction(Vector<double> x, Matrix<double> J)
{
if (J == (Matrix<double>) null)
J = (Matrix<double>) Matrix.Create<double>(3, 2);
if (this.val_F <= 80.0)
FModel.GetFJacobianOne(ref x, ref J);
if (this.val_F > 80.0 && this.val_F <= 380.0)
FModel.GetFJacobianTwo(ref x, ref J);
if (this.val_F > 380.0)
FModel.GetFJacobianThree(ref x, ref J);
if (this.val_P <= 240.0)
PModel.GetPJacobianOne(ref x, ref J);
if (this.val_P > 240.0 && this.val_P <= 350.0)
PModel.GetPJacobianTwo(ref x, ref J);
if (this.val_P > 350.0)
PModel.GetPJacobianThree(ref x, ref J);
if (this.val_B <= 240.0)
BModel.GetBJacobianOne(ref x, ref J);
if (this.val_B > 240.0)
BModel.GetBJacobianTwo(ref x, ref J);
return J;
}

public List<double> SubstituteValue(double val_X, double val_Y)
{
List<double> doubleList = new List<double>();
if (this.val_F <= 80.0)
doubleList.Add(Convert.ToDouble(new Expression(FModel.modelF1)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_F > 80.0 && this.val_F <= 380.0)
doubleList.Add(Convert.ToDouble(new Expression(FModel.modelF2)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_F > 380.0)
doubleList.Add(Convert.ToDouble(new Expression(FModel.modelF3)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_P <= 240.0)
doubleList.Add(Convert.ToDouble(new Expression(PModel.modelP1)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_P > 240.0 &&
this.val_P <= 350.0)
doubleList.Add(Convert.ToDouble(new Expression(PModel.modelP2)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_P > 350.0)
doubleList.Add(Convert.ToDouble(new Expression(PModel.modelP3)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_B <= 240.0)
doubleList.Add(Convert.ToDouble(new Expression(BModel.modelB1)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
if (this.val_B > 240.0)
doubleList.Add(Convert.ToDouble(new Expression(BModel.modelB2)
{
Parameters = {
["x"] = ((object) val_X),
["y"] = ((object) val_Y)
}
}.Evaluate()));
return doubleList;
}
}
}

using Extreme.Mathematics;

namespace RecipeSoftware
{
internal class FModel
{
public static string modelF1 = "388 - 1479.9 * x - 920.1 * y + 1600.1 * x * y + 1599.9 * Pow(x, 2) + 1000.1 * Pow(y, 2)";
public static string modelF2 = "440.81 - 1022.388 * x - 757.82 * y";
public static string modelF3 = "1221.67 - 5309.76 * x - 5149.04 * y";

public static void GetFModelOne(double value, ref Vector<double> x, ref Vector<double> f)
{
double num = 388.0 - value;
f[0] = num - 1479.9 * x[0] - 920.1 * x[1] + 1600.1 * x[0] * x[1] + 1599.9 * x[0] * x[0] + 1000.1 * x[1] * x[1];
}

public static void GetFJacobianOne(ref Vector<double> x, ref Matrix<double> J)
{
J[0, 0] = 3199.8 * x[0] + 1600.1 * x[1] - 1479.9;
J[0, 1] = 1600.1 * x[0] + 2000.2 * x[1] - 920.1;
}

public static void GetFModelTwo(double value, ref Vector<double> x, ref Vector<double> f)
{
double num = 440.81 - value;
f[0] = num - 1022.388 * x[0] - 757.82 * x[1];
}

public static void GetFJacobianTwo(ref Vector<double> x, ref Matrix<double> J)
{
J[0, 0] = -1022.388;
J[0, 1] = -757.82;
}

public static void GetFModelThree(double value, ref Vector<double> x, ref Vector<double> f)
{
double num = 1221.67 - value;
f[0] = num - 5309.76 * x[0] - 5149.04 * x[1];
}

public static void GetFJacobianThree(ref Vector<double> x, ref Matrix<double> J)
{
J[0, 0] = -5309.76;
J[0, 1] = -5149.04;
}
}
}


