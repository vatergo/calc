private void textBox_KeyPress(object sender, KeyPressEventArgs e)
{
char keyChar = e.KeyChar;
if (char.IsDigit(keyChar) || keyChar == '\b' || keyChar == '.')
return;
e.Handled = true;
}

private void trackBar1_Scroll(object sender, EventArgs e)
{
this.labelX.Text = (Convert.ToDouble(this.trackBar1.Value) / 1000.0).ToString();
List<double> doubleList = this.contrKonkr.SubstituteValue(double.Parse(this.labelX.Text), double.Parse(this.labelY.Text));
this.labelF.Text = Math.Round(doubleList[0], 4).ToString();
this.labelP.Text = Math.Round(doubleList[1], 4).ToString();
this.labelB.Text = Math.Round(doubleList[2], 4).ToString();
}

private void trackBar2_Scroll(object sender, EventArgs e)
{
this.labelY.Text = (Convert.ToDouble(this.trackBar2.Value) / 1000.0).ToString();
List<double> doubleList = this.contrKonkr.SubstituteValue(double.Parse(this.labelX.Text), double.Parse(this.labelY.Text));
this.labelF.Text = Math.Round(doubleList[0], 4).ToString();
this.labelP.Text = Math.Round(doubleList[1], 4).ToString();
this.labelB.Text = Math.Round(doubleList[2], 4).ToString();
}

private void textBox_TextChanged(TextBox txtBox)
{
if (!(txtBox.Text.Trim() != "") || Convert.ToDouble(Regex.Match(txtBox.Text, "\\d+").Value) <= 500.0)
return;
int num = (int) MessageBox.Show("Значение для расчетов не может быть более 500!");
txtBox.Text = "500.0";
}

