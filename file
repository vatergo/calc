private async void button1_Click(object sender, EventArgs e)
{
if (this.textBox2.Text.Length > 0 && this.textBox3.Text.Length > 0 && this.textBox4.Text.Length > 0)
{
CultureInfo temp_culture = Thread.CurrentThread.CurrentCulture;
Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("en-US");
this.contrKonkr.val_F = double.Parse(this.textBox2.Text);
this.contrKonkr.val_P = double.Parse(this.textBox3.Text);
this.contrKonkr.val_B = double.Parse(this.textBox4.Text);
Thread.CurrentThread.CurrentCulture = temp_culture;
LeastSquaresOptimizer trr = (LeastSquaresOptimizer) new TrustRegionReflectiveOptimizer();
trr = await this.contrKonkr.FindSolutionAsync();
List<double> list = this.contrKonkr.SubstituteValue(trr.Minimum[0], trr.Minimum[1]);
this.labelF.Text = Math.Round(list[0], 3).ToString();
this.labelP.Text = Math.Round(list[1], 3).ToString();
this.labelB.Text = Math.Round(list[2], 3).ToString();
Label labelX = this.labelX;
double num = Math.Round(trr.Minimum[0], 4);
string str1 = num.ToString();
labelX.Text = str1;
Label labelY = this.labelY;
num = Math.Round(trr.Minimum[1], 4);
string str2 = num.ToString();
labelY.Text = str2;
this.trackBar1.Value = Convert.ToInt32(trr.Minimum[0] * 1000.0);
this.trackBar2.Value = Convert.ToInt32(trr.Minimum[1] * 1000.0);
this.groupBox6.Visible = true;
temp_culture = (CultureInfo) null;
trr = (LeastSquaresOptimizer) null;
list = (List<double>) null;
}
else
{
int num1 = (int) MessageBox.Show("Заполните все необходимые поля.");
}
}

private List<double> PushList(double val, double val_max)
{
List<double> doubleList = new List<double>();
int num = 0;
while (val != val_max)
{
if (val + (double) num > val_max || val + (double) num == val_max)
{
doubleList.Add(val_max);
goto label_6;
}
else
{
doubleList.Add(val + (double) num);
num += 5;
}
}
doubleList.Add(val_max);
label_6:
return doubleList;
}

public async Task<List<Log» SubstituteDiapason(
ProgressBar progressBar,
List<double> listF,
List<double> listP,
List<double> listB)
{
List<Log> lg_lst = new List<Log>();
for (int i = 0; i < listF.Count; ++i)
{
for (int j = 0; j < listP.Count; ++j)
{
for (int k = 0; k < listB.Count; ++k)
{
this.contrDiap.val_F = listF[i];
this.contrDiap.val_P = listP[j];
this.contrDiap.val_B = listB[k];
LeastSquaresOptimizer trr = (LeastSquaresOptimizer) new TrustRegionReflectiveOptimizer();
trr = await this.contrDiap.FindSolutionAsync();
List<double> l_sol = this.contrDiap.SubstituteValue(trr.Minimum[0], trr.Minimum[1]);
Log log = new Log(Math.Round(listF[i], 4), Math.Round(listP[j], 4), Math.Round(listB[k], 4), trr.Minimum[0], trr.Minimum[1], Math.Round(l_sol[0], 4), Math.Round(l_sol[1], 4), Math.Round(l_sol[2], 4));
lg_lst.Add(log);
progressBar.Increment(1);
trr = (LeastSquaresOptimizer) null;
l_sol = (List<double>) null;
log = (Log) null;
}
}
}
return lg_lst;
}

private async void button2_Click(object sender, EventArgs e)
{
if (this.textBox5.Text.Length > 0 && this.textBox6.Text.Length > 0 && this.textBox7.Text.Length > 0 && this.textBox8.Text.Length > 0 && this.textBox9.Text.Length > 0 && this.textBox10.Text.Length > 0 && this.textBox12.Text.Length > 0 && this.textBox13.Text.Length > 0 &&
this.checkDiapason(this.textBox5, this.textBox6, "фильтрации") && this.checkDiapason(this.textBox7, this.textBox8, "растекаемости") && this.checkDiapason(this.textBox9, this.textBox10, "вязскости") && this.checkDiapason20(this.textBox5, this.textBox6, "фильтрации") && this.checkDiapason20(this.textBox7, this.textBox8, "растекаемости") && this.checkDiapason20(this.textBox9, this.textBox10, "вязскости"))
{
this.button3.Enabled = false;
this.button4.Enabled = false;
this.label8.Visible = true;
this.progressBar1.Visible = true;
this.textBox1.Clear();
this.progressBar1.Value = 0;
this.log_list.Clear();
CultureInfo temp_culture = Thread.CurrentThread.CurrentCulture;
Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("en-US");
double valF = double.Parse(this.textBox5.Text);
double valFMax = double.Parse(this.textBox6.Text);
double valP = double.Parse(this.textBox7.Text);
double valPMax = double.Parse(this.textBox8.Text);
double valB = double.Parse(this.textBox9.Text);
double valBMax = double.Parse(this.textBox10.Text);
Thread.CurrentThread.CurrentCulture = temp_culture;
List<double> listF = this.PushList(valF, valFMax);
List<double> listP = this.PushList(valP, valPMax);
List<double> listB = this.PushList(valB, valBMax);
int count = 0;
for (int i = 0; i < listF.Count; ++i)
{
for (int j = 0; j < listP.Count; ++j)
{
for (int k = 0; k < listB.Count; ++k)
++count;
}
}
this.progressBar1.Maximum = count;
this.progressBar1.Step = 1;
Form1 form1 = this;
List<Log> logList1 = form1.log_list;
List<Log> logList = await this.SubstituteDiapason(this.progressBar1, listF, listP, listB);
form1.log_list = logList;
form1 = (Form1) null;
logList = (List<Log>) null;
int num1 = (int) MessageBox.Show("Диапазон расчитан!");
this.label8.Visible = false;
this.progressBar1.Visible = false;
List<double> minSumList = new List<double>();
List<double> minCenaList = new List<double>();
double min = 999999.0;
double minSum = 999999.0;
double sumRes = 0.0;
foreach (Log l in this.log_list)
{
if (min > l.SUM)
{
min = l.SUM;
minSumList.Clear();
minSumList.Add(l.val_X);
minSumList.Add(l.val_Y);
valF = l.val_F;
valP = l.val_P;
valB = l.val_B;
}
if (minSum > this.contrDiap.saleX * l.val_X + this.contrDiap.saleY * l.val_Y)
{
minSum = this.contrDiap.saleX * l.val_X + this.contrDiap.saleY * l.val_Y;
sumRes = minSum;
minCenaList.Clear();
minCenaList.Add(l.val_X);
minCenaList.Add(l.val_Y);
}
TextBox textBox1_1 = this.textBox1;
textBox1_1.Text = textBox1_1.Text + "----------------------------" + Environment.NewLine;
TextBox textBox1_2 = this.textBox1;
textBox1_2.Text = textBox1_2.Text + "Ф: " + (object) Math.Round(l.val_F, 4) + ", Р: " + (object) Math.Round(l.val_P, 4) + ", В: " + (object) Math.Round(l.val_B, 4) + Environment.NewLine;
TextBox textBox1_3 = this.textBox1;
textBox1_3.Text = textBox1_3.Text + "ПФ: " + (object) Math.Round(l.val_X, 4) + Environment.NewLine;
TextBox textBox1_4 = this.textBox1;
textBox1_4.Text = textBox1_4.Text + "ПЛ: " + (object) Math.Round(l.val_Y, 4) + Environment.NewLine;
TextBox textBox1_5 = this.textBox1;
textBox1_5.Text = textBox1_5.Text + "Ф: " + (object) Math.Round(l.val_resF, 4) + Environment.NewLine;
TextBox textBox1_6 = this.textBox1;
textBox1_6.Text = textBox1_6.Text + "Р: " + (object) Math.Round(l.val_resP, 4) + Environment.NewLine;
TextBox textBox1_7 = this.textBox1;
textBox1_7.Text = textBox1_7.Text + "В: " + (object) Math.Round(l.val_resB, 4) + Environment.NewLine;
TextBox textBox1_8 = this.textBox1;
textBox1_8.Text = textBox1_8.Text + "----------------------------" + Environment.NewLine;
}
this.contrDiap.val_F = valF;
this.contrDiap.val_P = valP;
this.contrDiap.val_B = valB;
TextBox textBox1_9 = this.textBox1;
textBox1_9.Text = textBox1_9.Text + "Минимальные значения Пф и Пл: " + Environment.NewLine;
TextBox textBox1_10 = this.textBox1;
textBox1_10.Text = textBox1_10.Text + (object) Math.Round(minSumList[0], 4) + " - " + (object)
Math.Round(minSumList[1], 4) + Environment.NewLine;
List<double> listResSum = this.contrDiap.SubstituteValue(minSumList[0], minSumList[1]);
List<double> listCenaSum = this.contrDiap.SubstituteValue(minCenaList[0], minCenaList[1]);
TextBox textBox1_11 = this.textBox1;
textBox1_11.Text = textBox1_11.Text + "Ф: " + (object) Math.Round(listResSum[0], 3) + Environment.NewLine;
TextBox textBox1_12 = this.textBox1;
textBox1_12.Text = textBox1_12.Text + "Р: " + (object) Math.Round(listResSum[1], 3) + Environment.NewLine;
TextBox textBox1_13 = this.textBox1;
textBox1_13.Text = textBox1_13.Text + "В: " + (object) Math.Round(listResSum[2], 3) + Environment.NewLine;
Label labelFd = this.labelFD;
double num2 = Math.Round(listResSum[0], 3);
string str1 = num2.ToString();
labelFd.Text = str1;
Label labelPd = this.labelPD;
num2 = Math.Round(listResSum[1], 3);
string str2 = num2.ToString();
labelPd.Text = str2;
Label labelBd = this.labelBD;
num2 = Math.Round(listResSum[2], 3);
string str3 = num2.ToString();
labelBd.Text = str3;
Label labelXd = this.labelXD;
num2 = Math.Round(minSumList[0], 4);
string str4 = num2.ToString();
labelXd.Text = str4;
Label labelYd = this.labelYD;
num2 = Math.Round(minSumList[1], 4);
string str5 = num2.ToString();
labelYd.Text = str5;
Label labelFdc = this.labelFDC;
num2 = Math.Round(listCenaSum[0], 3);
string str6 = num2.ToString();
labelFdc.Text = str6;
Label labelPdc = this.labelPDC;
num2 = Math.Round(listCenaSum[1], 3);
string str7 = num2.ToString();
labelPdc.Text = str7;
Label labelBdc = this.labelBDC;
num2 = Math.Round(listCenaSum[2], 3);
string str8 = num2.ToString();
labelBdc.Text = str8;
Label labelXdc = this.labelXDC;
num2 = Math.Round(minCenaList[0], 4);
string str9 = num2.ToString();
labelXdc.Text = str9;
Label labelYdc = this.labelYDC;
num2 = Math.Round(minCenaList[1], 4);
string str10 = num2.ToString();
labelYdc.Text = str10;
Label labelSum = this.labelSUM;
num2 = Math.Round(sumRes, 4);
string str11 = num2.ToString() + " руб.";
labelSum.Text = str11;
this.button3.Enabled = true;
temp_culture = (CultureInfo) null;
listF = (List<double>) null;
listP = (List<double>) null;
listB = (List<double>) null;
minSumList = (List<double>) null;
minCenaList = (List<double>) null;
listResSum = (List<double>) null;
listCenaSum = (List<double>) null;
}
else
{
int num = (int) MessageBox.Show("Заполните все необходимые поля или проверьте корректность заполенния диапазонов.");
}
}

private bool checkDiapason(TextBox txtBox1, TextBox txtBox2, string svoistvo)
{
if (txtBox1.Text.Length <= 0 || txtBox2.Text.Length <= 0)
return false;
CultureInfo currentCulture = Thread.CurrentThread.CurrentCulture;
Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("en-US");
double num1 = double.Parse(txtBox1.Text);
double num2 = double.Parse(txtBox2.Text);
Thread.CurrentThread.CurrentCulture = currentCulture;
if (num1 <= num2)
return true;
int num3 = (int) MessageBox.Show("Значение " + svoistvo + " слева, не может быть больше значения справа!");
return false;
}

private bool checkDiapason20(TextBox txtBox1, TextBox txtBox2, string svoistvo)
{
if (txtBox1.Text.Length <= 0 || txtBox2.Text.Length <= 0)
return false;
CultureInfo currentCulture = Thread.CurrentThread.CurrentCulture;
Thread.CurrentThread.CurrentCulture = CultureInfo.CreateSpecificCulture("en-US");
double num1 = double.Parse(txtBox1.Text);
double num2 = double.Parse(txtBox2.Text);
Thread.CurrentThread.CurrentCulture = currentCulture;
if (num2 - num1 <= 20.0)
return true;
int num3 = (int) MessageBox.Show("Разница между правым и левым значением, не может быть более 20.");
return false;
}

private void button3_Click(object sender, EventArgs e)
{
if (this.comboBox1.SelectedItem != null && this.textBox11.Text.Length > 0 && this.log_list.Count > 0)
{
CultureInfo currentCulture = Thread.CurrentThread.CurrentCulture;
Thread.CurrentThread.CurrentCulture =
CultureInfo.CreateSpecificCulture("en-US");
double num = double.Parse(this.textBox11.Text);
Thread.CurrentThread.CurrentCulture = currentCulture;
List<Log> logList = new List<Log>();
if ("Фильтрация" == this.comboBox1.SelectedItem.ToString())
{
foreach (Log log1 in this.log_list)
{
if (log1.val_resF > num - Convert.ToDouble(this.numericUpDown1.Value) && log1.val_resF < num + Convert.ToDouble(this.numericUpDown1.Value))
{
Log log2 = new Log(log1.val_F, log1.val_P, log1.val_B, log1.val_X, log1.val_Y, log1.val_resF, log1.val_resP, log1.val_resB);
logList.Add(log2);
}
}
}
if ("Растекаемость" == this.comboBox1.SelectedItem.ToString())
{
foreach (Log log3 in this.log_list)
{
if (log3.val_resP > num - Convert.ToDouble(this.numericUpDown1.Value) && log3.val_resP < num + Convert.ToDouble(this.numericUpDown1.Value))
{
Log log4 = new Log(log3.val_F, log3.val_P, log3.val_B, log3.val_X, log3.val_Y, log3.val_resF, log3.val_resP, log3.val_resB);
logList.Add(log4);
}
}
}
if ("Вязкость" == this.comboBox1.SelectedItem.ToString())
{
foreach (Log log5 in this.log_list)
{
if (log5.val_resB > num - Convert.ToDouble(this.numericUpDown1.Value) && log5.val_resB < num + Convert.ToDouble(this.numericUpDown1.Value))
{
Log log6 = new Log(log5.val_F, log5.val_P, log5.val_B, log5.val_X, log5.val_Y, log5.val_resF, log5.val_resP, log5.val_resB);
logList.Add(log6);
}
}
}
this.textBox1.Clear();
foreach (Log log in logList)
{
TextBox textBox1_1 = this.textBox1;
textBox1_1.Text = textBox1_1.Text + "---------------------" + Environment.NewLine;
TextBox textBox1_2 = this.textBox1;
textBox1_2.Text = textBox1_2.Text + "Ф: " + (object) Math.Round(log.val_F, 4) + ", Р: " + (object) Math.Round(log.val_P, 4) + ", В: " + (object) Math.Round(log.val_B, 4) + Environment.NewLine;
TextBox textBox1_3 = this.textBox1;
textBox1_3.Text = textBox1_3.Text + "ПФ: " + (object) Math.Round(log.val_X, 4) + Environment.NewLine;
TextBox textBox1_4 = this.textBox1;
textBox1_4.Text = textBox1_4.Text + "ПЛ: " + (object) Math.Round(log.val_Y, 4) + Environment.NewLine;
TextBox textBox1_5 = this.textBox1;
textBox1_5.Text = textBox1_5.Text + "Ф: " + (object) Math.Round(log.val_resF, 4) + Environment.NewLine;
TextBox textBox1_6 = this.textBox1;
textBox1_6.Text = textBox1_6.Text + "Р: " + (object) Math.Round(log.val_resP, 4) + Environment.NewLine;
TextBox textBox1_7 = this.textBox1;
textBox1_7.Text = textBox1_7.Text + "В: " + (object) Math.Round(log.val_resB, 4) + Environment.NewLine;
TextBox textBox1_8 = this.textBox1;
textBox1_8.Text = textBox1_8.Text + "---------------------" + Environment.NewLine;
}
this.button4.Enabled = true;
}
else
{
int num1 = (int) MessageBox.Show("Имеются не заполненные поля или отсутствует список решений для поиска.");
}
}

